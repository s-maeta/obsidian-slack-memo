{
  "name": "obsidian-slack-sync",
  "version": "1.0.0",
  "description": "Obsidian plugin to synchronize Slack messages with your vault",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"docs/**/*.md\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"docs/**/*.md\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "clean": "rimraf main.js *.js.map coverage/",
    "precommit": "npm run lint && npm run typecheck && npm run test",
    "prepare": "npm run build"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "slack",
    "synchronization",
    "markdown"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "8.18.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "obsidian": "latest",
    "prettier": "^2.7.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "@types/uuid": "^9.0.0"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "date-fns": "^2.29.3",
    "js-yaml": "^4.1.0",
    "uuid": "^9.0.0"
  }
}
